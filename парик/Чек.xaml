        <TextBox x:Name="fiokl" HorizontalAlignment="Left" Margin="1637,174,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="246" Background="White" FontSize="20"/>
        <DatePicker x:Name="daaa" HorizontalAlignment="Left" Margin="1637,260,0,0" VerticalAlignment="Top" Height="38" Width="246" Background="White" FontSize="20"/>
        <Label Content="Дата" HorizontalAlignment="Left" Margin="1637,215,0,0" VerticalAlignment="Top" FontSize="22" Foreground="#DDFFFFFF"/>
        <Button Content="Выбрать услугу" HorizontalAlignment="Left" Margin="1680,374,0,0" VerticalAlignment="Top" FontSize="20" Height="46" Click="Button_Click_1"/>
        <Button Content="Вернуться" HorizontalAlignment="Left" Margin="1662,956,0,0" VerticalAlignment="Top" FontSize="22" Height="48" Width="171" Click="Button_Click" Cursor="Hand"/>
        <Label Content="Клиенты" HorizontalAlignment="Left" Margin="491,55,0,0" VerticalAlignment="Top" FontSize="48" Foreground="#DDFFFFFF"/>
        <Label Content="Чеки" HorizontalAlignment="Left" Margin="530,515,0,0" VerticalAlignment="Top" FontSize="48" Foreground="#DDFFFFFF"/>
        <TextBox x:Name="fioId" HorizontalAlignment="Left" Margin="1583,174,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="26" FontSize="20" Background="White"/>
        <TextBox x:Name="kolvv" HorizontalAlignment="Left" Margin="1583,515,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Background="White"/>

    </Grid>
</Window>
