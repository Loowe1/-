<Window x:Class="парик.Чек"
        WindowState="Maximized"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:парик"
        mc:Ignorable="d"
        Title="Чек" Height="1080" Width="1920" Loaded="Window_Loaded">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/1621817238_15-phonoteka_org-p-fon-s-parikmakherskimi-instrumentami-17.jpg"/>
        </Grid.Background>
        <Label Content="Чек" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="48" Foreground="#DDFFFFFF"/>
        <DataGrid x:Name="kl" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,132,674,554" CanUserAddRows="False" PreviewMouseDoubleClick="kl_PreviewMouseDoubleClick"/>
        <DataGrid x:Name="chek" d:ItemsSource="{d:SampleData ItemCount=5}" CanUserAddRows="False" Margin="10,592,674,10"/>
